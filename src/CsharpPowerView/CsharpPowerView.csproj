<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Version>1.0.0</Version>
		<Company>Nexus-Network-Cloud</Company>
		<RepositoryUrl>https://github.com/nexus-network-cloud/csharp-powerview</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageId>CsharpPowerView</PackageId>
		<Authors>Morkeon</Authors>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RootNamespace>NexusNetworkCloud.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>

</Project>
